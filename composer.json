{
    "name": "spatie/laravel-migrate-fresh",
    "description": "A command to build up the database from scratch",
	"homepage": "https://github.com/spatie/laravel-migrate-fresh",
    "keywords": [
        "spatie",
        "migrate",
        "fresh",
        "eloquent",
        "database",
        "laravel-migrate-fresh",
		"laravel",
		"database",
		"migration"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/console": "^5.3.28",
        "illuminate/events": "^5.3.28",
		"laravel/framework": "5.*"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.7",
        "orchestra/testbench": "^3.3",
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\MigrateFresh\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\MigrateFresh\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
  }
}
